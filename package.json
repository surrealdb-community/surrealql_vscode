{
  "name": "surrealql",
  "displayName": "surrealql",
  "description": "Language support for SurrealQL",
  "icon": "icons/surreal.png",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/surrealdb-community/surrealql_vscode.git"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Snippets"
  ],
  "publisher": "mathe42",
  "contributes": {
    "languages": [
      {
        "id": "surrealql",
        "extensions": [
          ".suql",
          ".srql",
          ".surql",
          ".surealql",
          ".surreal"
        ],
        "aliases": [
          "surrealql",
          "surrealdb"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/surreal.png",
          "dark": "./icons/surreal.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "surrealql",
        "scopeName": "source.surealql",
        "path": "./syntaxes/surrealql.json",
        "embeddedLanguages": {
          "source.js": "javascript"
        }
      },
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "source.vue",
          "text.html.markdown"
        ],
        "scopeName": "inline.surrealql-lit",
        "path": "./syntaxes/surrealql-lit.json",
        "embeddedLanguages": {
          "meta.embedded.block.surrealql": "surrealql"
        }
      }
    ],
    "snippets": [
      {
        "language": "surrealql",
        "path": "./snippets/surrealql.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "8.0.0",
    "@types/mocha": "10.0.0",
    "@types/node": "16.18.90",
    "@types/vscode": "1.71.0",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "@vscode/test-electron": "2.1.5",
    "eslint": "8.24.0",
    "glob": "8.0.3",
    "mocha": "10.0.0",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2"
  }
}
